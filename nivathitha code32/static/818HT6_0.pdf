                    });

                }, function() { });
                function waitUntilExists(isReady, success, error, count, interval){
                    if (count === undefined) {
                        count = 300;
                    }
                    if (interval === undefined) {
                        interval = 20;
                    }
                    if (isReady()) {
                        success();
                        return;
                    }
                    setTimeout(function(){
                        if (!count) {
                            if (error !== undefined) {
                                error();
                            }
                        } else {
